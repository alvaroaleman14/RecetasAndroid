<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/Boton_InsertarPlato"
        android:layout_width="130dp"
        android:layout_height="84dp"
        android:layout_marginStart="116dp"
        android:layout_marginTop="148dp"
        android:onClick="Select_InsertarPlato"
        android:text="Insertar Plato"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/Boton_GenerarMenu"
        android:layout_width="130dp"
        android:layout_height="84dp"
        android:layout_marginStart="116dp"
        android:layout_marginTop="56dp"
        android:onClick="Select_GenerarMenu"
        android:text="Generar Menu"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Boton_InsertarPlato" />

</androidx.constraintlayout.widget.ConstraintLayout>